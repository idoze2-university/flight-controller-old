             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>

        <TextBlock HorizontalAlignment="Left" Height="20" Margin="558,38,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="89" FontFamily="Agency FB"><Run FontWeight="Bold" Text="Internal Pitch (deg)"/></TextBlock>
